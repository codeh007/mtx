{
  "name": "gomtm-cli",
  "private": false,
  "version": "0.6.220",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": ["bin", "dist", "src"],
  "bin": {
    "gomtm": "./bin/gomtm"
  },
  "sideEffects": false,
  "scripts": {
    "build--": "bun build src/cli/main.ts --outdir=dist/gomtm-cli.js --target node",
    "build": "next build ",
    "wrangler_deploy": "bun run withEnv -- wrangler deploy src/entry-cfworker.ts --config=../../../gomtm/wrangler.mtmgw.json",
    "wrangler_dev": "bun run withEnv -- wrangler dev src/entry-cfworker.ts --config=../../../gomtm/wrangler.mtmgw.json --ip=0.0.0.0 --port=8788 --tsconfig=tsconfig.wrangler.json"
  },
  "dependencies": {
    "@codemirror/lang-sql": "^6.8.0",
    "get-port": "^7.1.0",
    "mtxuilib": "*",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redaxios": "^0.5.1",
    "superjson": "^2.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "vite": "^6.0.3",
    "@types/node": "^22",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwind-scrollbar": "^3.1.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7"
  },
  "other": {
    "styled-jsx": "^5.1.6",
    "commander": "^12.1.0"

  }
}
