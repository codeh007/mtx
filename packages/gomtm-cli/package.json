{
  "name": "gomtm-cli",
  "private": false,
  "version": "0.6.335",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": ["bin", "dist", "src"],
  "bin": {
    "gomtm": "./bin/gomtm"
  },
  "sideEffects": false,
  "scripts": {
    "build--": "bun build src/cli/main.ts --outdir=dist/gomtm-cli.js --target node",
    "build": "next build ",
    "wrangler_deploy": "bun run withEnv -- wrangler deploy src/entry-cfworker.ts --config=../../../gomtm/wrangler.mtmgw.json",
    "wrangler_dev": "bun run withEnv -- wrangler dev src/entry-cfworker.ts --config=../../../gomtm/wrangler.mtmgw.json --ip=0.0.0.0 --port=8788 --tsconfig=tsconfig.wrangler.json"
  },
  "dependencies": {
    "next": "^15.2",
    "react": "^19",
    "react-dom": "^19",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "vite": "^6.2.5",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.7",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7"
  },
  "other": {
    "styled-jsx": "^5.1.6",
    "commander": "^12.1.0",
    "@codemirror/lang-sql": "^6.8.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "redaxios": "^0.5.1",
    "superjson": "^2.2.2",
    "mtxuilib": "*"
  }
}
