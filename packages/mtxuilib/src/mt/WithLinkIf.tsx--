"use client";

// import { MtLink } from "mtxuilib/mt/mtlink";
import type { PropsWithChildren } from "react";
import React from "react";

export const WithLinkIf = (
  props: {
    href?: string;
  } & PropsWithChildren,
) => {
  const { href, children } = props;
  if (href) {
    return <MtLink href={href}>{children}</MtLink>;
  }
  return children;
};
