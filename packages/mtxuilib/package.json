  "name": "mtxuilib",
  "private": false,
  "version": "0.6.10",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": ["dist"],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "repository": {
    "url": "https://github.com/codeh007/mtxuilib"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*.ts",
        "src/*.tsx",
        "src/*/index.ts",
        "src/*/index.tsx",
        "dist/*.d.ts",
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./dist/index.js", "./dist/index.d.ts"],
      "import": ["./src/index.ts", "./dist/index.js", "./dist/*"]
    },
    "./*": {
      "types": [
        "./src/*",
        "./src/*.tsx",
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx",
        "./dist/*.d.ts",
        "./dist/*/index.d.ts"
      ],
      "import": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js",
        "./dist/*"
      ],
      "default": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js",
        "./dist/*"
      ]
    },
    "./styles/globals.css": {
      "import": ["./src/styles/globals.css", "./dist/styles/globals.css"],
      "require": ["./src/styles/globals.css", "./dist/styles/globals.css"]
    },
    "./styles/*": {
      "import": ["./src/styles/*", "./dist/styles/*"],
      "require": ["./src/styles/*", "./dist/styles/*"]
    }
  },
  "scripts": {
    "dev": "npx next dev",
    "build": "bun run ./src/cli/main.ts build",
    "withEnv": "dotenv -e ../../../gomtm/env/mtmaiui.env --",
    "gen": "bun src/cli/main.ts gen",
    "wrangler_deploy": "bun run withEnv -- wrangler deploy src/entry-cfworker.ts --config=../../../gomtm/wrangler.mtmgw.json",
    "wrangler_dev": "bun run withEnv -- wrangler dev src/entry-cfworker.ts --config=../../../gomtm/wrangler.mtmgw.json --ip=0.0.0.0 --port=8787 --tsconfig=tsconfig.wrangler.json"
  },
