{
  "name": "mtremotion",
  "private": false,
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": ["dist"],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "repository": {
    "url": "https://github.com/codeh007/mtxuilib"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*.ts",
        "src/*.tsx",
        "src/*/index.ts",
        "src/*/index.tsx",
        "dist/*.d.ts",
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./dist/index.js", "./dist/index.d.ts"],
      "import": ["./src/index.ts", "./dist/index.js", "./dist/*"]
    },
    "./*": {
      "types": [
        "./src/*",
        "./src/*.tsx",
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx",
        "./dist/*.d.ts",
        "./dist/*/index.d.ts"
      ],
      "import": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js",
        "./dist/*"
      ],
      "default": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js",
        "./dist/*"
      ]
    },
    "./styles/globals.css": {
      "import": ["./src/styles/globals.css", "./dist/styles/globals.css"],
      "require": ["./src/styles/globals.css", "./dist/styles/globals.css"]
    },
    "./styles/*": {
      "import": ["./src/styles/*", "./dist/styles/*"],
      "require": ["./src/styles/*", "./dist/styles/*"]
    }
  },
  "description": "Basic project template for Motion Canvas",
  "author": "motion-canvas",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build--": "next build",
    "start": "next start",
    "lint": "next lint",
    "remotion": "remotionb studio",
    "render": "remotionb render",
    "deploy": "node deploy.mjs"
  },
  "dependencies": {
    "mtxuilib": "*",
    "@remotion/animated-emoji": "^4",
    "@remotion/animation-utils": "^4",
    "@remotion/bundler": "^4",
    "@remotion/cli": "^4",
    "@remotion/enable-scss": "^4",
    "@remotion/fonts": "^4",
    "@remotion/google-fonts": "^4",
    "@remotion/install-whisper-cpp": "^4",
    "@remotion/lambda": "^4",
    "@remotion/layout-utils": "^4",
    "@remotion/lottie": "^4",
    "@remotion/media-utils": "^4",
    "@remotion/motion-blur": "^4",
    "@remotion/noise": "^4",
    "@remotion/paths": "^4",
    "@remotion/player": "^4",
    "@remotion/preload": "^4",
    "@remotion/shapes": "^4",
    "@remotion/tailwind-v4": "^4",
    "clsx": "^2.1",
    "next": "^15.2",
    "react": "19",
    "react-dom": "19",
    "remotion": "^4",
    "tailwind-merge": "^3",
    "zod": "^3.22"
  },
  "devDependencies": {
    "@remotion/eslint-plugin": "^4",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web": "0.0.166",
    "autoprefixer": "^10",
    "eslint": "^9",
    "@next/eslint-plugin-next": "^15",
    "eslint-config-next": "^15",
    "postcss": "^8.4",
    "prettier": "^3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
