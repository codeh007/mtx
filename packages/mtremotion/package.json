{
  "name": "mtremotion",
  "private": false,
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": ["dist"],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "repository": {
    "url": "https://github.com/codeh007/mtxuilib"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*.ts",
        "src/*.tsx",
        "src/*/index.ts",
        "src/*/index.tsx",
        "dist/*.d.ts",
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./dist/index.js", "./dist/index.d.ts"],
      "import": ["./src/index.ts", "./dist/index.js", "./dist/*"]
    },
    "./*": {
      "types": [
        "./src/*",
        "./src/*.tsx",
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx",
        "./dist/*.d.ts",
        "./dist/*/index.d.ts"
      ],
      "import": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js",
        "./dist/*"
      ],
      "default": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js",
        "./dist/*"
      ]
    },
    "./styles/globals.css": {
      "import": ["./src/styles/globals.css", "./dist/styles/globals.css"],
      "require": ["./src/styles/globals.css", "./dist/styles/globals.css"]
    },
    "./styles/*": {
      "import": ["./src/styles/*", "./dist/styles/*"],
      "require": ["./src/styles/*", "./dist/styles/*"]
    }
  },
  "description": "Basic project template for Motion Canvas",
  "author": "motion-canvas",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build--": "next build",
    "start": "next start",
    "lint": "next lint",
    "remotion": "remotionb studio",
    "render": "remotionb render",
    "deploy": "node deploy.mjs"
  },
  "dependencies": {
    "@remotion/animated-emoji": "4.0.298",
    "@remotion/animation-utils": "4.0.298",
    "@remotion/bundler": "4.0.298",
    "@remotion/cli": "4.0.298",
    "@remotion/cloudrun": "4.0.298",
    "@remotion/enable-scss": "4.0.298",
    "@remotion/fonts": "4.0.298",
    "@remotion/gif": "4.0.298",
    "@remotion/google-fonts": "4.0.298",
    "@remotion/install-whisper-cpp": "4.0.298",
    "@remotion/lambda": "4.0.298",
    "@remotion/layout-utils": "4.0.298",
    "@remotion/lottie": "4.0.298",
    "@remotion/mcp": "4.0.298",
    "@remotion/media-utils": "4.0.298",
    "@remotion/motion-blur": "4.0.298",
    "@remotion/noise": "4.0.298",
    "@remotion/paths": "4.0.298",
    "@remotion/player": "4.0.298",
    "@remotion/preload": "4.0.298",
    "@remotion/shapes": "4.0.298",
    "@remotion/tailwind-v4": "4.0.298",
    "clsx": "2.1.1",
    "next": "15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "remotion": "4.0.298",
    "tailwind-merge": "3.0.1",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@remotion/eslint-plugin": "4.0.298",
    "@eslint/eslintrc": "3.1.0",
    "@types/node": "20.12.14",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/web": "0.0.166",
    "autoprefixer": "10.4.20",
    "dotenv": "16.0.3",
    "eslint": "9.19.0",
    "@next/eslint-plugin-next": "15.1.6",
    "eslint-config-next": "15.1.6",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "tailwindcss": "4.0.3",
    "typescript": "5.8.2"
  }
}
