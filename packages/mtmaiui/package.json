{
  "name": "mtmaiui",
  "private": false,
  "version": "0.5.27",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": ["dist", "src"],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "dev_vite": "vite --port=6111",
    "withEnv": "dotenv -e ../../../gomtm/env/mtmaiui.env --",
    "dev": "dotenv -e ../../../gomtm/env/mtmaiui.env -- next dev",
    "dev:next": "next dev",
    "build": "vite build && next build",
    "build:tsc": "tsc --outDir dist --jsx react-jsx --incremental false --declaration true --noEmit false --removeComments true --project tsconfig.json",
    "build_vite_dev": "vite build --config vite.config.dev.js",
    "build:types": "vite build --types",
    "serve": "vite preview",
    "start": "vite",
    "db:gen": "drizzle-kit generate",
    "db:migrate": "bun run withEnv -- bun run src/db/migrate.ts",
    "db:push": "bun run withEnv -- drizzle-kit push"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*.ts",
        "src/*.tsx",
        "src/*/index.ts",
        "src/*/index.tsx",
        "dist/*.d.ts",
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./dist/index.js", "./dist/index.d.ts"],
      "import": ["./src/index.ts", "./dist/index.js", "./dist/*"]
    },
    "./components/*/*": {
      "types": "./dist/components/*/*.d.ts",
      "import": "./dist/components/*/*.js",
      "require": "./dist/components/*/*.js"
    },
    "./*": {
      "types": [
        "./src/*.tsx",
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx",
        "./dist/*.d.ts",
        "./dist/*/index.d.ts"
      ],
      "import": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js"
      ]
    }
  },
  "dependencies": {
    "@codemirror/lang-sql": "^6.8.0",
    "@tanstack/react-query": "^5.62.14",
    "@tanstack/react-query-devtools": "^5.62.14",
    "@tanstack/react-query-next-experimental": "^5.62.14",
    "@tanstack/react-router": "^1.93.0",
    "@tanstack/router-devtools": "^1.93.0",
    "@tanstack/router-plugin": "^1.93.0",
    "@tanstack/start": "^1.93.0",
    "get-port": "^7.1.0",
    "mtmaiapi": "*",
    "mtxuilib": "*",
    "next": "^15.1.5",
    "react": "^19",
    "react-dom": "^19",
    "redaxios": "^0.5.1",
    "superjson": "^2.2.2",
    "unenv": "^1.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/generator": "^7.26.3",
    "@cloudflare/workers-types": "^4.20240423.0",
    "@lezer/highlight": "^1.2.0",
    "@next/bundle-analyzer": "^15.1.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/bun": "^1.1.6",
    "@types/cors": "^2.8.17",
    "@types/diff": "^5.2.1",
    "@types/express": "^4.17.21",
    "@types/jquery": "^3.5.30",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/sass": "^1.45.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cloudflare": "^3.5.0",
    "compression": "^1.7.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.30.1",
    "eventsource-parser": "^0.1.0",
    "express": "^4.21.2",
    "isbot": "^5.1.17",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.49",
    "postcss-preset-env": "^9.5.9",
    "rehype-autolink-headings": "^7.1.0",
    "serve-static": "^1.16.2",
    "shiki": "^1.9.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.5.5",
    "tailwind-scrollbar": "^3.1.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7",
    "unist-util-visit": "^4.1.2",
    "vite-plugin-ssr": "^0.4.142"
  }
}
