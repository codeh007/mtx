{
  "name": "workspaces",
  "version": "0.0.758",
  "license": "MIT",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": ["dist"],
  "scripts": {
    "build": "turbo build",
    "with-env": "dotenv -e ./env/gomtm.env --",
    "gen": "cd packages/mtmaiapi && bun run gen",
    "release": "changeset publish"
  },
  "typesVersions": {
    "*": {
      "*": ["src/*.ts", "src/*.tsx", "src/*/index.ts", "src/*/index.tsx"]
    }
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./dist/esm/index.js"],
      "import": ["./src/index.ts", "./dist/esm/index.js"],
      "require": ["./dist/esm/index.js"]
    },
    "./*": {
      "types": [
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx",
        "./dist/*.js",
        "./dist/*/index.js"
      ],
      "import": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*.js",
        "./dist/*/index.js"
      ]
    }
  },
  "dependencies": {
    "@langchain/groq": "^0.1.2",
    "next": "^15.1.6",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.11",
    "@changesets/get-release-plan": "4.0.6",
    "@cloudflare/next-on-pages": "^1.13.7",
    "@cloudflare/workers-types": "^4.20250109.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@octokit/rest": "^21.0.0",
    "@types/bun": "^1.1.16",
    "@types/node": "22.7.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "bun-types": "^1.1.43",
    "changeset": "^0.2.6",
    "express": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7",
    "vercel": "^39.3.0",
    "vite": "^6.0.3",
    "wrangler": "^3.105.1"
  },
  "resolutions": {
    "prosemirror-model": "1.23.0"
  },
  "overrides": {
    "prosemirror-model": "1.23.0"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "bun@1.1.20",
  "workspaces": ["packages/*", "apps/*"]
}
