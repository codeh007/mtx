{
  "name": "workspaces",
  "version": "0.0.757",
  "license": "MIT",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": ["dist"],
  "bin": {
    "gomtm": "./bin/gomtm",
    "gomtm2": "./dist/main.js"
  },
  "scripts": {
    "build": "turbo build",
    "with-env": "dotenv -e ./env/gomtm.env --",
    "gen": "cd packages/mtmaiapi && bun run gen"
  },
  "typesVersions": {
    "*": {
      "*": ["src/*.ts", "src/*.tsx", "src/*/index.ts", "src/*/index.tsx"]
    }
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./dist/esm/index.js"],
      "import": ["./src/index.ts", "./dist/esm/index.js"],
      "require": ["./dist/esm/index.js"]
    },
    "./*": {
      "types": [
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx",
        "./dist/*.js",
        "./dist/*/index.js"
      ],
      "import": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*.js",
        "./dist/*/index.js"
      ]
    }
  },
  "dependencies": {
    "@langchain/groq": "^0.1.2",
    "next": "^15.1.3",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/bun": "^1.1.6",
    "@cloudflare/workers-types": "^4.20241216.0",
    "express": "^4.19.2",
    "vite": "^6.0.3",
    "wrangler": "^3.88.0",
    "@cloudflare/next-on-pages": "^1.13.7",
    "@biomejs/biome": "^1.9.3",
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22",
    "bun-types": "^1.1.36",
    "changeset": "^0.2.6",
    "turbo": "^2.0.12",
    "typescript": "^5.7"
  },
  "resolutions": {
    "prosemirror-model": "1.23.0"
  },
  "overrides": {
    "prosemirror-model": "1.23.0"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "bun@1.1.20",
  "workspaces": ["packages/*", "apps/*"]
}
