{
  "name": "mtapppium",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./src/index.tsx", "./dist/index.js"],
      "import": ["./src/index.ts", "./src/index.tsx", "./dist/index.js"]
    },
    "./*": {
      "types": ["./src/*.ts", "./src/*/index.ts", "./src/*.tsx", "./src/*/index.tsx"],
      "import": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*.js",
        "./dist/*/index.js"
      ],
      "require": [
        "./dist/*.js",
        "./dist/*/index.js",
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx"
      ]
    }
  },
  "files": ["dist"],
  "bin": {
    "mtmaiadmin": "dist/main.js"
  },
  "scripts": {
    "test": "node src/test.js",
    "start": "export ANDROID_HOME=/usr/lib/android-sdk/platform-tools && appium"
  },
  "dependencies": {
    "appium": "^2.18.0"
  },
  "devDependencies": {
    "appium-uiautomator2-driver": "^4.2.3",
    "webdriverio": "^9.14.0"
  }
}
