{
  "name": "workflows-starter",
  "main": "src/entry-cfworker.ts",
  "compatibility_date": "2024-10-22",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "logs": {
      "enabled": true
    }
  },
  "workflows": [
    {
      "name": "workflows-starter",
      "binding": "MY_WORKFLOW",
      "class_name": "MyWorkflow"
    },
    {
      "name": "workflows-demo",
      "binding": "WORKFLOW_DEMO",
      "class_name": "WorkflowDemo"
    },
    {
      "name": "WfHelloToolUse",
      "binding": "WfHelloToolUse",
      "class_name": "WfHelloToolUse"
    },
    {
      "name": "WfHelloAssisant",
      "binding": "WfHelloAssisant",
      "class_name": "WfHelloAssisant"
    }
  ],
  "send_email": [
    {
      "name": "SEND_EMAIL"
    }
  ]
}
